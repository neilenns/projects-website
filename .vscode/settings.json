{
  "files.exclude": {
    ".hugo_build.lock": true,
    "public/": true,
    "resources/": true,
    "package*.json": true
  },
  "files.associations": {
    "*.svg.html": "plaintext"
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    ".gitignore": ".gitattributes, .hugoignore",
    "hugo.yaml": ".markdownlint.json, go.mod, go.sum, .coderabbit.yml, .prettierrc.json, netlify.toml",
    "eslint.config.js": ".markdownlint.jsonc",
    "README.md": "DEVELOPMENT.md"
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    }
  },
  "cSpell.words": [
    "ATIS",
    "callsign",
    "callsigns",
    "discordtimestamp",
    "Elgato",
    "KBFI",
    "KGRF",
    "KHIO",
    "KOLM",
    "KPAE",
    "KPDX",
    "KRNT",
    "KSEA",
    "KTCM",
    "KTIW",
    "KUAO",
    "LIFR",
    "MVFR",
    "ogimage",
    "opengraph",
    "streamdeck",
    "trackaudio",
    "vatis",
    "VATSIM",
    "Verdana"
  ],
  "cSpell.enabledFileTypes": {
    ".svg.html": false
  }
}
